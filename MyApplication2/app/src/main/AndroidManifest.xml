<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hackcu.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".calibration_activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_calibration_activity"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".util.gesturesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_gestures"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".GesturesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_gestures"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".util.Gesture"
            android:label="@string/title_activity_gesture" >
        </activity>
        <activity
            android:name=".Gesture2"
            android:label="@string/title_activity_gesture2" >
        </activity>
    </application>

</manifest>
